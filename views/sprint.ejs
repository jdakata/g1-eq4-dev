<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('head.ejs') %>
</head>
<body>

<ul class="list-unstyled">
    <% sprint.USList.forEach(function(US) { %>
        <div class="card" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">ID: X </h5>
                <p class="card-text">Description : <%= US.description %> -
                    Difficulté : <%= US.difficulty %></p>
                <a href="#" class="btn btn-primary stretched-link">Détails</a>
            </div>
        </div>
    <% }); %>
</ul>

<footer>
    <%- include('footer.ejs') %>
</footer>

</body>
</html>